<Window x:Class="devector.MainWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:devector"
		mc:Ignorable="d"
		Title="MainWindow" Height="489" Width="816">
	<Grid RenderTransformOrigin="0.618,0.1">
		<StackPanel>
			<Menu Width="816">
				<MenuItem Header="File">
					<MenuItem Header="Open" Click="MenuItem_Open"/>
					<MenuItem x:Name="recent_files_menu_item" Header="Recent Files"/>
				</MenuItem>
				<MenuItem Header="Tools">
					<MenuItem Header="Debugger" Click="debugger_menu_item_click"/>
					<MenuItem Header="Memory Map" Click="memory_map_menu_click"/>
				</MenuItem>
			</Menu>
			<Image x:Name="picture_display" Stretch="Fill"/>
		</StackPanel>
	</Grid>
</Window>
